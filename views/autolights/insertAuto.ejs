<% include ../partials/header %>

<div class="container">
    <h2 align="center">Εισαγωγή Στοιχείων</h2>
    <form action="/autoLighthouses/insertAuto/<%= autoLightHouse._id %>?_method=PUT" method="POST">
        <div class="row">
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΑΕΦ</span>
					<input type="text" class="form-control" name="aef" id="aef" value="<%= autoLightHouse.aef %>">
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΟΝΟΜΑΣΙΑ</span>
					<input type="text" class="form-control" name="lighthouse" id="lighthouse" value="<%= autoLightHouse.lighthouse %>">
				</div>
			</div>
			<div class="col-md-4 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΧΡΩΜΑΤΙΣΜΟΣ</span>
					<input type="text" class="form-control" name="colour" id="colour" value="<%= autoLightHouse.colour %>">
				</div>
			</div>
			<div class="col-md-4 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΤΟΜΕΙΣ</span>
					<input type="text" class="form-control" name="sections" id="sections" value="<%= autoLightHouse.sections %>">
				</div>
			</div>
			<div class="col-md-4 mt-0">
