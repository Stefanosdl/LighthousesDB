<% include ../partials/header %>

<div class="container">
    <h2 align="center">Εισαγωγή Στοιχείων</h2>
    <form action="/ledLighthouses/insertLed/<%= ledLightHouse._id %>?_method=PUT" method="POST">
        <div class="row">
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΑΕΦ</span>
					<input type="text" class="form-control" name="aef" id="aef" value="<%= ledLightHouse.aef %>">
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΟΝΟΜΑΣΙΑ</span>
					<input type="text" class="form-control" name="lighthouse" id="lighthouse" value="<%= ledLightHouse.lighthouse %>">
				</div>
			</div>
			<div class="col-md-4 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΧΡΩΜΑΤΙΣΜΟΣ</span>
					<input type="text" class="form-control" list="colours" name="colour" id="colour" value="<%= ledLightHouse.colour %>">
					<datalist id="colours">
						<% if(storeroom != undefined) { %>
							<% for (var i=0; i < storeroom.colours.length; i++) { %>
								<option value="<%= storeroom.colours[i] %>">
							<% } %>
						<% } %>
					</datalist>
				</div>
			</div>
			<div class="col-md-4 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΤΟΜΕΙΣ</span>
					<input type="text" class="form-control" name="sections" id="sections" value="<%= ledLightHouse.sections %>">
				</div>
			</div>
			<div class="col-md-4 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΧΑΡΑΚΤΗΡΙΣΤΙΚΟ</span>
					<input type="text" class="form-control" name="features" id="features" value="<%= ledLightHouse.features %>">
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΠΕΡΙΓΡΑΦΗ ΘΕΣΕΩΣ/ΤΟΠΟΘΕΤΗΣΗΣ</span>
					<input type="text" class="form-control" name="position" id="position" value="<%= ledLightHouse.position %>">
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΠΕΡΙΟΧΗ</span>
					<input type="text" class="form-control" list="locations" name="location" id="location" value="<%= ledLightHouse.location %>">
					<datalist id="locations">
						<% if(storeroom != undefined) { %>
							<% for (var i=0; i < storeroom.locations.length; i++) { %>
								<option value="<%= storeroom.locations[i] %>">
							<% } %>
						<% } %>
					</datalist>
				</div>
			</div>
			<div class="col-md-6 mt-0" id="fieldListSolar">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΗΛΙΑΚΗ ΓΕΝΝΗΤΡΙΑ</span>
					<input type="text" class="form-control" list="datalistOptions1" name="solarGeneratorNew" id="solarGeneratorNew">
					<datalist id="datalistOptions1">
						<% if(storeroom != undefined) { %>
							<% for (var i=0; i < storeroom.solarGenerators.length; i++) { %>
								<option value="<%= storeroom.solarGenerators[i] %>">
							<% } %>
						<% } %>
					</datalist>
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
					<input type="date" class="form-control" name="solarGeneratorDateNew" id="solarGeneratorDateNew">
				</div>
			</div>
			<% for (var i=0; i < ledLightHouse.solarGenerator.length; i++) { %>
				<div class="col-md-6 mt-0" id="fieldListSolar">
					<div class="input-group input-group-sm mb-3">
						<span class="input-group-text" id="inputGroup-sizing-sm">ΗΛΙΑΚΗ ΓΕΝΝΗΤΡΙΑ</span>
						<input type="text" class="form-control" list="datalistOptions1" name="solarGenerator[]" id="solarGenerator" value="<%= ledLightHouse.solarGenerator[i] %>">
						<datalist id="datalistOptions1">
							<% if(storeroom != undefined) { %>
								<% for (var i=0; i < storeroom.solarGenerators.length; i++) { %>
									<option value="<%= storeroom.solarGenerators[i] %>">
								<% } %>
							<% } %>
						</datalist>
					</div>
				</div>
				<% if(ledLightHouse.solarGeneratorDateGroups.get(i.toString()) != undefined) { %>
					<div class="col-md-6 mt-0">
						<div class="input-group input-group-sm mb-3">
							<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
						<input type="date" class="form-control" name="solarGeneratorDate[]" id="solarGeneratorDate" value="<%= ledLightHouse.solarGeneratorDateGroups.get(i.toString())[ledLightHouse.solarGeneratorDateGroups.get(i.toString()).length-1] %>">
					</div>
					</div>
				<% } else { %>
					<div class="col-md-6 mt-0">
						<div class="input-group input-group-sm mb-3">
							<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
							<input type="date" class="form-control" name="solarGeneratorDate[]" id="solarGeneratorDate">
						</div>
					</div>
				<% } %>
			<% } %>
			<div class="col-md-6 mt-0" id="fieldListAcc">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΣΥΣΣΩΡΕΥΤΗΣ</span>
					<input type="text" class="form-control" list="datalistOptions" name="accumulatorNew" id="accumulator">
					<datalist id="datalistOptions">
						<% if(storeroom != undefined) { %>
							<% for (var i=0; i < storeroom.accumulators.length; i++) { %>
								<option value="<%= storeroom.accumulators[i] %>">
							<% } %>
						<% } %>
					</datalist>
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
					<input type="date" class="form-control" name="accumulatorDateNew" id="accumulatorDateNew">
				</div>
			</div>
			<% for (var i=0; i < ledLightHouse.accumulator.length; i++) { %>
				<div class="col-md-6 mt-0" id="fieldListAcc">
					<div class="input-group input-group-sm mb-3">
						<span class="input-group-text" id="inputGroup-sizing-sm">ΣΥΣΣΩΡΕΥΤΗΣ</span>
						<input type="text" class="form-control" list="datalistOptions" name="accumulator[]" id="accumulator" value="<%= ledLightHouse.accumulator[i] %>">
						<datalist id="datalistOptions">
							<% if(storeroom != undefined) { %>
								<% for (var i=0; i < storeroom.accumulators.length; i++) { %>
									<option value="<%= storeroom.accumulators[i] %>">
								<% } %>
							<% } %>
						</datalist>
					</div>
				</div>
				<% if(ledLightHouse.accumulatorDateGroups.get(i.toString()) != undefined) { %>
					<div class="col-md-6 mt-0">
						<div class="input-group input-group-sm mb-3">
							<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
							<input type="date" class="form-control" name="accumulatorDate[]" id="accumulatorDate" value="<%= ledLightHouse.accumulatorDateGroups.get(i.toString())[ledLightHouse.accumulatorDateGroups.get(i.toString()).length-1] %>">
						</div>
					</div>
				<% } else { %>
					<div class="col-md-6 mt-0">
						<div class="input-group input-group-sm mb-3">
							<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
							<input type="date" class="form-control" name="accumulatorDate[]" id="accumulatorDate">
						</div>
					</div>
				<% } %>
			<% } %>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΚΕΦΑΛΗ</span>
					<input type="text" class="form-control" list="heads" name="head" id="head" value="<%= ledLightHouse.head %>">
					<datalist id="heads">
						<% if(storeroom != undefined) { %>
							<% for (var i=0; i < storeroom.heads.length; i++) { %>
								<option value="<%= storeroom.heads[i] %>">
							<% } %>
						<% } %>
					</datalist>
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
					<input type="date" class="form-control" name="headDate" id="headDate" value="<%= ledLightHouse.headDate.pop() %>">
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΡΥΘΜΙΣΤΗΣ ΦΟΡΤΙΣΕΩΣ/ ΦΟΡΤΙΣΤΗΣ</span>
					<input type="text" class="form-control" list="generatorSockets" name="chargeRegulator" id="chargeRegulator" value="<%= ledLightHouse.chargeRegulator %>">
					<datalist id="generatorSockets">
						<% if(storeroom != undefined) { %>
							<% for (var i=0; i < storeroom.generatorSockets.length; i++) { %>
								<option value="<%= storeroom.generatorSockets[i] %>">
							<% } %>
						<% } %>
					</datalist>
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
					<input type="date" class="form-control" name="chargeRegulatorDate" id="chargeRegulatorDate" value="<%= ledLightHouse.chargeRegulatorDate.pop() %>">
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΥΠΟΔΟΧΗ</span>
					<input type="text" class="form-control" list="torchSockets" name="socket" id="socket" value="<%= ledLightHouse.socket %>">
					<datalist id="torchSockets">
						<% if(storeroom != undefined) { %>
							<% for (var i=0; i < storeroom.torchSockets.length; i++) { %>
								<option value="<%= storeroom.torchSockets[i] %>">
							<% } %>
						<% } %>
					</datalist>
				</div>
			</div>
			<div class="col-md-6 mt-0">
				<div class="input-group input-group-sm mb-3">
					<span class="input-group-text" id="inputGroup-sizing-sm">ΗΜ/ΝΙΑ ΤΟΠΟΘΕΤΗΣΗΣ</span>
					<input type="date" class="form-control" name="socketDate" id="socketDate" value="<%= ledLightHouse.socketDate.pop() %>">
				</div>
			</div>
        </div>
        
        <br>
        
		<div class="text-center">
			<div class="btn-group" role="group" aria-label="Basic checkbox toggle button group">
				<input type="checkbox" class="btn-check" id="btncheck1" autocomplete="off">
				<a onclick="window.history.back()" class="btn btn-outline-primary float-left">Πίσω</a>
				<a href="/" class="btn btn-outline-primary float-left">Αρχική Σελίδα</a>
                <button class="btn btn-outline-primary float-right" type="submit">Εισαγωγή στοιχείων</button>
			</div>
		</div>
    </form>
</div>
<br>

<% include ../partials/footer %>
